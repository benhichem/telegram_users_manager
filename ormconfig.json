{
  "type": "sqlite",
  "database": "database.sqlite",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/data/entities/data/**/*.entity.ts"
  ],
  "migrations": [
    "src/data/migrations/**/*.ts"
  ],
  "subscribers": [
    "src/data/subscribers/**/*.subscriber.ts"
  ],
  "cli": {
    "entitiesDir": "src/entities",
    "migrationsDir": "src/migrations",
    "subscribersDir": "src/subscribers"
  }
}